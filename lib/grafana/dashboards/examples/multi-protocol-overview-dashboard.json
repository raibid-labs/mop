{
  "dashboard": {
    "title": "Multi-Protocol Overview - All Examples",
    "tags": ["obi", "overview", "multi-protocol", "examples"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 0},
        "type": "row",
        "title": "Overall System Health",
        "collapsed": false
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 6, "x": 0, "y": 2},
        "type": "stat",
        "title": "HTTP API - Request Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"http-api\"}[5m]))",
            "legendFormat": "RPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 6, "w": 6, "x": 6, "y": 2},
        "type": "stat",
        "title": "gRPC Service - Request Rate",
        "targets": [
          {
            "expr": "sum(rate(grpc_server_handled_total{service=\"grpc-service\"}[5m]))",
            "legendFormat": "RPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 6, "w": 6, "x": 12, "y": 2},
        "type": "stat",
        "title": "SQL App - Query Rate",
        "targets": [
          {
            "expr": "sum(rate(sql_queries_total{service=\"sql-app\"}[5m]))",
            "legendFormat": "QPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "qps",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 50, "color": "yellow"},
                {"value": 200, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 6, "w": 6, "x": 18, "y": 2},
        "type": "stat",
        "title": "Redis Cache - Operation Rate",
        "targets": [
          {
            "expr": "sum(rate(redis_commands_total{service=\"redis-cache\"}[5m]))",
            "legendFormat": "OPS",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 6,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 8},
        "type": "stat",
        "title": "Kafka Streaming - Message Rate",
        "targets": [
          {
            "expr": "sum(rate(kafka_messages_consumed_total{service=\"kafka-streaming\"}[5m]))",
            "legendFormat": "msgs/s",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "msgs",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 5000, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "background",
          "graphMode": "area"
        }
      },
      {
        "id": 7,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 8},
        "type": "gauge",
        "title": "Overall System Health Score",
        "targets": [
          {
            "expr": "avg((1 - (sum(rate(http_requests_total{status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total[5m])))) * 100)",
            "legendFormat": "Health Score",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 8,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 14},
        "type": "row",
        "title": "Error Rates by Service",
        "collapsed": false
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16},
        "type": "timeseries",
        "title": "Error Rate Comparison",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{service=\"http-api\",status_code=~\"5..\"}[5m])) / sum(rate(http_requests_total{service=\"http-api\"}[5m])) * 100",
            "legendFormat": "HTTP API",
            "refId": "A"
          },
          {
            "expr": "sum(rate(grpc_server_handled_total{service=\"grpc-service\",grpc_code!=\"OK\"}[5m])) / sum(rate(grpc_server_handled_total{service=\"grpc-service\"}[5m])) * 100",
            "legendFormat": "gRPC Service",
            "refId": "B"
          },
          {
            "expr": "sum(rate(sql_query_errors_total{service=\"sql-app\"}[5m])) / sum(rate(sql_queries_total{service=\"sql-app\"}[5m])) * 100",
            "legendFormat": "SQL App",
            "refId": "C"
          },
          {
            "expr": "sum(rate(redis_command_errors_total{service=\"redis-cache\"}[5m])) / sum(rate(redis_commands_total{service=\"redis-cache\"}[5m])) * 100",
            "legendFormat": "Redis Cache",
            "refId": "D"
          },
          {
            "expr": "sum(rate(kafka_processing_errors_total{service=\"kafka-streaming\"}[5m])) / sum(rate(kafka_messages_consumed_total{service=\"kafka-streaming\"}[5m])) * 100",
            "legendFormat": "Kafka Streaming",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 24},
        "type": "row",
        "title": "Latency Distribution",
        "collapsed": false
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "type": "timeseries",
        "title": "p95 Latency Comparison",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"http-api\"}[5m]))",
            "legendFormat": "HTTP API",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(grpc_server_handling_seconds_bucket{service=\"grpc-service\"}[5m]))",
            "legendFormat": "gRPC Service",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.95, rate(sql_query_duration_seconds_bucket{service=\"sql-app\"}[5m]))",
            "legendFormat": "SQL App",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.95, rate(redis_command_duration_seconds_bucket{service=\"redis-cache\"}[5m]))",
            "legendFormat": "Redis Cache",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.95, rate(kafka_processing_duration_seconds_bucket{service=\"kafka-streaming\"}[5m]))",
            "legendFormat": "Kafka Streaming",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "type": "timeseries",
        "title": "p99 Latency Comparison",
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{service=\"http-api\"}[5m]))",
            "legendFormat": "HTTP API",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(grpc_server_handling_seconds_bucket{service=\"grpc-service\"}[5m]))",
            "legendFormat": "gRPC Service",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(sql_query_duration_seconds_bucket{service=\"sql-app\"}[5m]))",
            "legendFormat": "SQL App",
            "refId": "C"
          },
          {
            "expr": "histogram_quantile(0.99, rate(redis_command_duration_seconds_bucket{service=\"redis-cache\"}[5m]))",
            "legendFormat": "Redis Cache",
            "refId": "D"
          },
          {
            "expr": "histogram_quantile(0.99, rate(kafka_processing_duration_seconds_bucket{service=\"kafka-streaming\"}[5m]))",
            "legendFormat": "Kafka Streaming",
            "refId": "E"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 34},
        "type": "row",
        "title": "Resource Utilization",
        "collapsed": false
      },
      {
        "id": 14,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 36},
        "type": "timeseries",
        "title": "CPU Usage by Service",
        "targets": [
          {
            "expr": "sum by (service) (rate(container_cpu_usage_seconds_total{service=~\"http-api|grpc-service|sql-app|redis-cache|kafka-streaming\"}[5m]))",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 15,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 36},
        "type": "timeseries",
        "title": "Memory Usage by Service",
        "targets": [
          {
            "expr": "sum by (service) (container_memory_usage_bytes{service=~\"http-api|grpc-service|sql-app|redis-cache|kafka-streaming\"})",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      },
      {
        "id": 16,
        "gridPos": {"h": 2, "w": 24, "x": 0, "y": 44},
        "type": "row",
        "title": "Distributed Tracing Overview",
        "collapsed": false
      },
      {
        "id": 17,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 46},
        "type": "timeseries",
        "title": "Trace Spans by Service",
        "targets": [
          {
            "expr": "sum by (service) (rate(traces_spanmetrics_calls_total{service=~\"http-api|grpc-service|sql-app|redis-cache|kafka-streaming\"}[5m]))",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "spans/s",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 20
            }
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "custom",
          "options": "http-api,grpc-service,sql-app,redis-cache,kafka-streaming",
          "label": "Service",
          "multi": true,
          "includeAll": true,
          "current": {
            "value": ["$__all"]
          }
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  }
}
